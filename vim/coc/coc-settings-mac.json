{
  "coc.preferences.formatOnSave": true,
  "diagnostic.checkCurrentLine": true,
  "codeLens.enable": true,
  "deno.enable": false,
  "colors.enable": true,
  "volar.disableFormatting": true,
  "prettier.enable": true,
  "prettier.disableLanguages": [
    "vue"
  ],
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "prettier.formatterPriority": 2,
  "eslint.autoFixOnSave": true,
  "eslint.fixOnSaveTimeout": 3000,
  "tsserver.enable": true,
  "tsserver.log": "verbose",
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "blade",
    "vue"
  ],
  "markdownlint.config": {
    "no-inline-html": false
  },
  "typescript.format.enable": true,
  "javascript.format.enable": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.format.semicolons": "remove",
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": true,
  "typescript.format.insertSpaceBeforeFunctionParenthesis": true,
  "clangd.path": "/usr/local/bin/clangd15",
  "emmet.excludeLanguages": [
    "php"
  ],
  "intelephense.phpunit.colors": true,
  "intelephense.phpunit.enableSplitRight": true,
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": [
        "config",
        "exec",
        "--",
        "ocamllsp"
      ],
      "filetypes": [
        "ocaml",
        "reason"
      ]
    }
  },
  "elixir.pathToElixirLS": "~/.elixir-ls/release/language_server.sh",
  "snippets.ultisnips.pythonPrompt": false,
  "intelephense.format.braces": "psr12"
}
