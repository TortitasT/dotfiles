{
  "coc.preferences.formatOnSave": true,
  "diagnostic.checkCurrentLine": true,
  "codeLens.enable": true,
  "deno.enable": false,
  "colors.enable": true,
  "prettier.enable": false,
  "prettier.disableLanguages": [],
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "none",
  "prettier.formatterPriority": 2,
  "eslint.autoFixOnSave": true,
  "tsserver.enable": true,
  "tsserver.log": "verbose",
  "html.format.templating": true,
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "eruby",
    "blade",
    "heex"
  ],
  "emmet.includeLanguages": {
    "eruby": "html",
    "blade": "html"
  },
  "solargraph.formatting": true,
  "markdownlint.config": {
    "no-inline-html": false
  },
  "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/17.0.3/clangd_17.0.3/bin/clangd",
  "emmet.excludeLanguages": [
    "php"
  ],
  "intelephense.phpunit.colors": true,
  "intelephense.phpunit.enableSplitRight": true,
  "intelephense.diagnostics.undefinedMethods": false,
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": [
        "config",
        "exec",
        "--",
        "ocamllsp"
      ],
      "filetypes": [
        "ocaml",
        "reason"
      ]
    },
    "kotlin": {
      "command": "~/.local/share/kotlin-language-server/bin/kotlin-language-server",
      "filetypes": [
        "kotlin"
      ]
    }
  },
  "snippets.ultisnips.pythonPrompt": false,
  "intelephense.format.braces": "psr12",
  "inlayHint.enable": false
}
